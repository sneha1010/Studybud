            "content": "{% extends 'main.html' %}\r\n\r\n{% block content %}\r\n<main class=\"auth layout\">\r\n\r\n  {% if page == 'login' %}\r\n  <div class=\"container\">\r\n    <div class=\"layout__box\">\r\n      <div class=\"layout__boxHeader\">\r\n        <div class=\"layout__boxTitle\">\r\n          <h3>Login</h3>\r\n        </div>\r\n      </div>\r\n      <div class=\"layout__body\">\r\n        <h2 class=\"auth__tagline\">Find your study partner</h2>\r\n\r\n        <form class=\"form\" action=\"\" method=\"POST\">\r\n          {% csrf_token %}\r\n          <div class=\"form__group form__group\">\r\n            <label for=\"room_name\">Email</label>\r\n            <input id=\"username\" name=\"email\" type=\"email\" placeholder=\"e.g. dennis_ivy@email.com \"/>\r\n         \r\n          <div class=\" form__group\">\r\n            <label for=\"password\">Password</label>\r\n            <input id=\"password\" name=\"password\" type=\"password\"\r\n              placeholder=\"&bull;&bull;&bull;&bull;&bull;&bull;&bull;&bull;\" />\r\n          </div>\r\n\r\n          <button class=\"btn btn--main\" type=\"submit\">\r\n            <svg version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" width=\"32\" height=\"32\" viewBox=\"0 0 32 32\">\r\n              <title>lock</title>\r\n              <path\r\n                d=\"M27 12h-1v-2c0-5.514-4.486-10-10-10s-10 4.486-10 10v2h-1c-0.553 0-1 0.447-1 1v18c0 0.553 0.447 1 1 1h22c0.553 0 1-0.447 1-1v-18c0-0.553-0.447-1-1-1zM8 10c0-4.411 3.589-8 8-8s8 3.589 8 8v2h-16v-2zM26 30h-20v-16h20v16z\">\r\n              </path>\r\n              <path\r\n                d=\"M15 21.694v4.306h2v-4.306c0.587-0.348 1-0.961 1-1.694 0-1.105-0.895-2-2-2s-2 0.895-2 2c0 0.732 0.413 1.345 1 1.694z\">\r\n              </path>\r\n            </svg>\r\n\r\n            Login\r\n          </button>\r\n        </form>\r\n\r\n        <div class=\"auth__action\">\r\n          <p>Haven't signed up yet?</p>\r\n          <a href=\"{% url 'register' %}\" class=\"btn btn--link\">Sign Up</a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  {% else %}\r\n  <div class=\"container\">\r\n    <div class=\"layout__box\">\r\n      <div class=\"layout__boxHeader\">\r\n        <div class=\"layout__boxTitle\">\r\n          <h3>Register</h3>\r\n        </div>\r\n      </div>\r\n      <div class=\"layout__body\">\r\n        <h2 class=\"auth__tagline\">Find your study partner</h2>\r\n\r\n        <form class=\"form\" action=\"\" method=\"POST\">\r\n          {% csrf_token %}\r\n\r\n          {% for field in form %}\r\n          <div class=\"form__group form__group\">\r\n            <label for=\"room_name\">{{field.label}}</label>\r\n            {{field}}\r\n          </div>\r\n          {% endfor %}\r\n\r\n\r\n          <button class=\"btn btn--main\" type=\"submit\">\r\n            <svg version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" width=\"32\" height=\"32\" viewBox=\"0 0 32 32\">\r\n              <title>lock</title>\r\n              <path\r\n                d=\"M27 12h-1v-2c0-5.514-4.486-10-10-10s-10 4.486-10 10v2h-1c-0.553 0-1 0.447-1 1v18c0 0.553 0.447 1 1 1h22c0.553 0 1-0.447 1-1v-18c0-0.553-0.447-1-1-1zM8 10c0-4.411 3.589-8 8-8s8 3.589 8 8v2h-16v-2zM26 30h-20v-16h20v16z\">\r\n              </path>\r\n              <path\r\n                d=\"M15 21.694v4.306h2v-4.306c0.587-0.348 1-0.961 1-1.694 0-1.105-0.895-2-2-2s-2 0.895-2 2c0 0.732 0.413 1.345 1 1.694z\">\r\n              </path>\r\n            </svg>\r\n\r\n            Register\r\n          </button>\r\n        </form>\r\n\r\n        <div class=\"auth__action\">\r\n          <p>Already signed up yet?</p>\r\n          <a href=\"{% url 'login' %}\" class=\"btn btn--link\">Sign Up</a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  {% endif %}\r\n</main>\r\n{% endblock content %}"
